    "Issuers": [
      "AuthServer"
    ],
    "Audience": [
      "AuthClient"
    ],
    "PRIVATE_KEY_PATH": "./keys/private_key.xml",
    "PUBLIC_KEY_PATH": "./keys/public_key.xml"
  },
